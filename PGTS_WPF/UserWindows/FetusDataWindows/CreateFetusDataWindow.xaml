<Window x:Class="PGTS_WPF.UserWindows.FetusDataWindows.CreateFetusDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PGTS_WPF.UserWindows.FetusDataWindows"
        mc:Ignorable="d"
        Style="{StaticResource MaterialDesignWindow}"
        WindowStartupLocation="CenterScreen"
        Title="Create Fetus Data" Height="400" Width="450">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="300">
            <TextBlock Text="Weight" Margin="0,0,0,10"/>
            <TextBox Name="txtWeight" Margin="0,0,0,10"/>
            <TextBlock Text="Height" Margin="0,0,0,10"/>
            <TextBox Name="txtHeight" Margin="0,0,0,10"/>
            <TextBlock Text="Head Circumference" Margin="0,0,0,10"/>
            <TextBox Name="txtHeadCircumference" Margin="0,0,0,10"/>
            <TextBlock Text="Date" Margin="0,0,0,10"/>
            <DatePicker Name="dpDate" Margin="0,0,0,10" />
            <Button Content="Create" Click="btnCreate_Click" Margin="0,0,0,10"/>
        </StackPanel>
    </Grid>
</Window>
